		String fname=request.getParameter("phone");
		String fname=request.getParameter("dob");
		String fname=request.getParameter("gender");
		String fname=request.getParameter("address");
		String fname=request.getParameter("city");
		String fname=request.getParameter("state");
		String fname=request.getParameter("pin");
		String fname=request.getParameter("");
		String fname=request.getParameter("firstName");
		String fname=request.getParameter("firstName");
		String fname=request.getParameter("firstName");
		
		
	}
}
